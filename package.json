{
  "name": "seder-18",
  "version": "1.1.1",
  "description": "CLI tool to manage JSON translations",
  "main": "src/cli.js",
  "bin": {
    "seder": "./src/cli.js"
  },
  "scripts": {
    "start": "node src/cli.js --dir ./src/example",
    "start:example": "node src/cli.js --dir ./src/example --main en --ignore fr.json",
    "build": "cd src/client && npm run build"
  },
  "files": [
    "src/cli.js",
    "src/client/dist",
    "src/backend/index.js",
    "src/backend/translations.js",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "i18n",
    "translations",
    "editor"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "minimist": "^1.2.8",
    "yargs": "^17.7.2"
  }
}
